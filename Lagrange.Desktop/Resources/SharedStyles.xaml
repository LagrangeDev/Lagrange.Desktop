<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:desktop="clr-namespace:Lagrange.Desktop">
    <desktop:DivideByConverter x:Key="DivideByConverter" />
    <desktop:DoubleToSeverityColorConverter x:Key="DoubleToSeverityColorConverter" />
    <desktop:RunStateToColorConverter x:Key="RunStateToColorConverter" />
    <desktop:RunStateToStringConverter x:Key="RunStateToStringConverter" />
    <!--#region TabItem-->
    <Style x:Key="TabItemBorderStyle" TargetType="Border">
        <Setter Property="Margin" Value="10" />
        <Setter Property="CornerRadius" Value="5" />
        <Setter Property="Background" Value="#9AFFFFFF" />
    </Style>
    <!--#endregion-->
</ResourceDictionary>